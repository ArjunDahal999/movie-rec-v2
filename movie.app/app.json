{
  "expo": {
    "name": "movie.app",
    "slug": "test",
    "version": "1.0.0",
    "scheme": "movie.app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/MainLogo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/MainLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/MainLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/MainLogo.png",
        "backgroundColor": "#000"
      },
      "package": "com.arzun_dhz.test"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9822423e-3a9d-4d58-9a4c-5d7e0ce941ca"
      }
    },
    "owner": "arzun_dhz"
  }
}
